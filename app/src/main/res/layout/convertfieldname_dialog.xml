<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:background="@color/dialog_bg_color" android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout android:orientation="vertical" android:background="@drawable/dialog_headbar_bg" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_margin="5dp">
        <TextView android:textColor="@color/dialog_txt_color" android:background="@drawable/dialog_headtext_corner_bg" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="字段转换列表:"/>
        <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_weight="1">
            <include android:id="@+id/table_fieldConvert" layout="@layout/mytable_listview"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/ll_bottomLL" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content">
            <CheckBox android:textColor="@color/dialog_txt_color" android:id="@+id/ck_ConvertToCode" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="5dp" android:text="转换为字典细类代码"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
